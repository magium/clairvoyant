{
  "name": "magium/clairvoyant",
  "description": "An API for reporting on PHPUnit and Magium tests at https://clairvoyant.magiumlib.com/",
  "minimum-stability": "stable",
  "license": "Apache2.0",
  "authors": [
    {
      "name": "Kevin Schroeder",
      "email": "kschroeder@magiumlib.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Magium\\Clairvoyant\\": "lib",
      "Magium\\Clairvoyant\\Tests\\": "tests"
    },
    "files": [
      "registration.php"
    ]
  },
  "require-dev": {
    "magium/magium": "^1"
  },
  "require": {
    "psr/log": "^1.0",
    "phpunit/phpunit": "^5|^6",
    "guzzlehttp/guzzle": "^6.3"
  },
  "suggest": {
    "magium/magium": "^1",
    "magium/clairvoyant-generic-tests": "^1"
  }
}
